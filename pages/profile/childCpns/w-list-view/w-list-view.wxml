<!--pages/profile/childCpns/w-list-view/w-list-view.wxml-->
<view class='list-view'>
  <block wx:for="{{infos}}" wx:key="index">
    <view class='list-item' bindtap="{{item.event}}">
      <image src="/assets/images/profile/{{item.icon}}"/>
      <text>{{item.info}}</text>
      <image class="more" src = "/assets/images/order/right.png"
        mode = "widthFix" />
    </view>
  </block>
</view>
