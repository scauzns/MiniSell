<!--pages/cart/childCpns/bottom-bar/bottom-bar.wxml-->
<view class='bottom-bar'>
  <view class='check-box' catchtap="onSelectAll">
    <w-check-icon icon="icon" isChecked="{{selected}}" />
    <view>全选</view>
  </view>
  <view class='counter'>
    总计: ¥{{price}}
  </view>
  <view class='payment' catchtap="payment">去结算({{counter}})</view>
</view>
